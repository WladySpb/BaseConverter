{
    "name": "WladySpb/BaseConverter",
    "description": "Convert numbers between any base you want, including negative numbers and floating point numbers",
    "keywords": [
        "numbers",
        "converter"
    ],
    "homepage": "https://github.com/WladySpb/BaseConverter",
    "license": "GPL-3.0 License",
    "authors": [
        {
            "name": "WladySpb",
            "email": "wladyspb85@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer" : "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "WladySpb\\BaseConverter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WladySpb\\BaseConverter\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
